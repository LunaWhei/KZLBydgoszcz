@model IEnumerable<KZLBydgoszcz.Models.Class_Name>

@{
    ViewData["Title"] = "Klasa";
}

<h1>Klasa</h1>

<p>
    <a asp-action="Create">Dodaj nową</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Class_identificator)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Class_identificator)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Student_classID">Edycja</a> |
                <a asp-action="Details" asp-route-id="@item.Student_classID">Szczegóły</a> |
                <a asp-action="Delete" asp-route-id="@item.Student_classID">Usuń</a>
            </td>
        </tr>
}
    </tbody>
</table>
